FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

mechanical
(
    steel
    {
        type            neoHookeanElasticMisesPlastic;
        rho             rho [ 1 -3 0 0 0 0 0 ] 7500;
        E               E [ 1 -1 -2 0 0 0 0 ] 206.9e6;
        nu              nu [ 0 0 0 0 0 0 0 ] 0.29;
        "file|fileName" "$FOAM_CASE/constant/plasticStrainVsYieldStress";
        outOfBounds     clamp;
	solveVertexCentredPressureEquation yes;
        numericalTangent yes;
        tangentEps 1e-10;
    }
);

