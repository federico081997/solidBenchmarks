FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    fixed
    {
        type            uniformFixedValue;
        uniformValue    ( 0 0 0 );
    }
    load
    {
        type            pointSolidTraction;
        tractionSeries
        {
            "fileName|file"    "$FOAM_CASE/constant/timeVsTraction";
            outOfBounds clamp;
        }
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
    top
    {
        type            empty;
    }
    bottom
    {
        type            empty;
    }
    front
    {
        type            pointSolidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
    back
    {
        type            pointSolidTraction;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
}

// ************************************************************************* //
